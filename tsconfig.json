{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    // "target": "es2015",
    // "module": "es2015",
    "strict": true,
    "jsx": "react-native",
    "baseUrl": ".", // this must be specified if "paths" is specified.
    "paths": {
      "@env": ["node_modules/<path to the module dist>"] // this mapping is relative to "baseUrl"
    },
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
